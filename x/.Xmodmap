!! swap right alt for control
remove Control = Control_R
remove Mod1 = Alt_R

! send Control on right Alt
keycode 108 = Control_R Control_R

! send Alt on PrtSc
keycode 107 = Alt_R Alt_R

! send PrtSc on right Ctrl
keycode 105 = Print Sys_Req Print Sys_Req

! (re) add modifiers
add Control = Control_R
add Mod1 = Alt_R

